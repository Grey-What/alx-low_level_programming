#include <stdio.h>
/**
 * main - this function print the whole alphabet in lower case, in reverse
 *
 * Description: this function prints the whole alphabet in lower case with the
 * putchar function in reverse
 *
 * Return: the main function returns a zero
 */
int main(void)
{
	char alpha = 'z';

	while (alpha <= 'a')
	{
		putchar(alpha);
		alpha++;
	}
	putchar('\n');
	return (0);
}
